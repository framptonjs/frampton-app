{
  "name": "frampton-app",
  "version": "0.0.8",
  "main": "./dist/frampton-app.js",
  "description": "A module for wiring apps with frampton.js",
  "scripts": {
    "test": "frampton test",
    "build": "frampton build",
    "clean-install": "rm -rf node_modules; npm install;",
    "release": "frampton release --patch",
    "minor-release": "frampton release --minor",
    "major-release": "frampton release --major",
    "docs": "rm -rf docs/* && jsdoc packages/**/src/*.js -d docs --readme README.md  --private"
  },
  "author": "Kevin Greene <hightimesteddy@gmail.com>",
  "license": "BSD",
  "repository": {
    "type": "git",
    "url": "git@github.com:framptonjs/frampton-app.git"
  },
  "bugs": {
    "url": "https://github.com/framptonjs/frampton-app/issues"
  },
  "homepage": "https://github.com/framptonjs/frampton-app",
  "dependencies": {
    "frampton": "0.3.5",
    "frampton-dom": "0.1.4"
  },
  "devDependencies": {
    "frampton-build": "0.0.7",
    "frampton-cli": "0.0.12",
    "jsdoc": "3.4.0"
  }
}